<div class="jumbotron">

  <div class="contained">
    <h1>Generated Biography</h1>
  </div>

  <div class="contained token">
    <strong>Token:</strong> <%= @token %>
  </div>

  <div class="pic-and-bio">
    <div class="pic"><img id="user-picture" src="<%= @pic_url %>"></div>
    <div class="bio">
      <%= @bio %>
    </div>
  </div>

</div>